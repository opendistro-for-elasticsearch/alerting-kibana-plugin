{
  "name": "opendistro-alerting",
  "version": "1.12.0.0",
  "description": "Kibana Alerting Plugin",
  "main": "index.js",
  "license": "Apache-2.0",
  "homepage": "https://github.com/opendistro-for-elasticsearch/alerting-kibana-plugin",
  "kibana": {
    "version": "7.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opendistro-for-elasticsearch/alerting-kibana-plugin"
  },
  "scripts": {
    "kbn": "node ../../scripts/kbn",
    "lint": "../../node_modules/.bin/eslint '**/*.js' -c .eslintrc --ignore-path .gitignore",
    "start": "plugin-helpers start",
    "test:jest": "../../node_modules/.bin/jest --config ./test/jest.config.js",
    "build": "yarn plugin-helpers build",
    "plugin-helpers": "node ../../scripts/plugin_helpers"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@elastic/eslint-import-resolver-kibana": "link:../../packages/kbn-eslint-import-resolver-kibana",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0"
  },
  "dependencies": {
    "brace": "0.11.1",
    "formik": "1.1.1",
    "lodash": "^4.17.20",
    "query-string": "^6.13.2",
    "react-router-dom": "^5.2.0",
    "react-vis": "^1.11.7"
  },
  "resolutions": {
    "fstream": "1.0.12"
  },
  "engines": {
    "node": "10.22.1",
    "yarn": "^1.21.1"
  }
}
